{
  "name": "rollupcordovavue",
  "displayName": "RollupCordovaVue",
  "version": "0.0.1",
  "description": "Rollup Cordova with splashscreen + VueJS with VueRouter and VueRessource + VueMaterial with a base application Template including a swipe left menu.",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev DIR_DEST=platforms/browser/www/ rollup -c -w",
    "server": "NODE_ENV=dev rollup -c && cordova run browser",
    "browser": "NODE_ENV=production rollup -c && cordova build browser",
    "android": "NODE_ENV=production rollup -c && cordova run android",
    "ios": "NODE_ENV=production rollup -c && cordova run ios"
  },
  "author": "b-remi",
  "license": "MIT",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {}
    },
    "platforms": []
  },
  "dependencies": {
    "cordova": "^8.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "devDependencies": {
    "rollup": "^0.55.3",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "vue": "^2.3.4",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-resource": "^1.3.4",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vue2-touch-events": "^1.0.0"
  }
}
